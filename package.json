{
  "name": "empress-blog",
  "version": "4.1.0",
  "description": "Fully-functional, SEO friendly static site implementation of a blog system built on Ember.",
  "keywords": [
    "ember-addon",
    "ember-ghost",
    "empress-blog",
    "prember-plugin"
  ],
  "repository": "https://github.com/empress/empress-blog",
  "license": "MIT",
  "author": {
    "name": "Chris Manson",
    "email": "chris.manson@mainmatter.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "ember-compatibility-tests": "ember try:each",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:node": "mocha node-tests"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@embroider/macros": "^1.19.2",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-plugin": "^4.0.3",
    "broccoli-static-site-json": "^4.2.0",
    "broccoli-static-site-json-xml": "^1.0.1",
    "case": "^1.6.1",
    "downsize-cjs": "^1.0.1",
    "ember-auto-import": "^2.8.1",
    "ember-body-class": "^3.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-get-config": "^2.1.1",
    "ember-meta": "^2.0.0",
    "ember-scroll": "^1.0.2",
    "empress-blueprint-helpers": "^1.2.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.20",
    "recast": "^0.23.4",
    "require-directory": "^2.1.1",
    "resolve": "^1.10.1",
    "walk-sync": "^3.0.0",
    "xml2js": "^0.6.2",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.0.0",
    "@ember/test-helpers": "^3.3.1",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.12.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.0",
    "ember-fetch": "^8.0.2",
    "ember-inflector": "^6.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^12.0.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^4.0.0",
    "empress-blog-casper-template": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.2.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "lint-to-the-future": "^2.0.0",
    "lint-to-the-future-eslint": "^2.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^6.1.4",
    "prember": "^2.0.0",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "^3.2.1",
    "release-plan": "^0.17.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "webpack": "^5.95.0"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "engines": {
    "node": "20.* || 22.* || >= 24"
  },
  "pnpm": {
    "overrides": {
      "sharp": "^0.34.4"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-meta",
      "ember-data"
    ],
    "before": "prember",
    "demoURL": "https://empress-blog-casper-template.netlify.com"
  },
  "fastbootDependencies": [
    "crypto"
  ]
}
